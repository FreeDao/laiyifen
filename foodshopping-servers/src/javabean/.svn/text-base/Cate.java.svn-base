package javabean;

public class Cate {
	int cate_id;  				//id
	String cate_name;			//名称
	int cate_inventory;			//库存
	int cate_sale_count;		//销售数量
	String cate_description;	//美食描述
	float cate_oldprice;		//原价
	float cate_price;			//现价
	int cate_image_id;			//美食图片
	int category_id;			//美食种类
	int flavor_category_id;		//美食口味种类
	String cate_sale_info;		//美食促销信息
	
	
	public Cate() {
		super();
	}
	public Cate(int cate_id, String cate_name, int cate_inventory,
			int cate_sale_count, String cate_description, float cate_oldprice,
			float cate_price, int cate_image_id, int category_id,
			int flavor_category_id, String cate_sale_info) {
		this.cate_id = cate_id;
		this.cate_name = cate_name;
		this.cate_inventory = cate_inventory;
		this.cate_sale_count = cate_sale_count;
		this.cate_description = cate_description;
		this.cate_oldprice = cate_oldprice;
		this.cate_price = cate_price;
		this.cate_image_id = cate_image_id;
		this.category_id = category_id;
		this.flavor_category_id = flavor_category_id;
		this.cate_sale_info = cate_sale_info;
	}
	public int getCate_id() {
		return cate_id;
	}
	public void setCate_id(int cate_id) {
		this.cate_id = cate_id;
	}
	public String getCate_name() {
		return cate_name;
	}
	public void setCate_name(String cate_name) {
		this.cate_name = cate_name;
	}
	public int getCate_inventory() {
		return cate_inventory;
	}
	public void setCate_inventory(int cate_inventory) {
		this.cate_inventory = cate_inventory;
	}
	public int getCate_sale_count() {
		return cate_sale_count;
	}
	public void setCate_sale_count(int cate_sale_count) {
		this.cate_sale_count = cate_sale_count;
	}
	public String getCate_description() {
		return cate_description;
	}
	public void setCate_description(String cate_description) {
		this.cate_description = cate_description;
	}
	public float getCate_oldprice() {
		return cate_oldprice;
	}
	public void setCate_oldprice(float cate_oldprice) {
		this.cate_oldprice = cate_oldprice;
	}
	public float getCate_price() {
		return cate_price;
	}
	public void setCate_price(float cate_price) {
		this.cate_price = cate_price;
	}
	public int getCate_image_id() {
		return cate_image_id;
	}
	public void setCate_image_id(int cate_image_id) {
		this.cate_image_id = cate_image_id;
	}
	public int getCategory_id() {
		return category_id;
	}
	public void setCategory_id(int category_id) {
		this.category_id = category_id;
	}
	public int getFlavor_category_id() {
		return flavor_category_id;
	}
	public void setFlavor_category_id(int flavor_category_id) {
		this.flavor_category_id = flavor_category_id;
	}
	public String getCate_sale_info() {
		return cate_sale_info;
	}
	public void setCate_sale_info(String cate_sale_info) {
		this.cate_sale_info = cate_sale_info;
	}
	@Override
	public String toString() {
		return "Cate [cate_id=" + cate_id + ", cate_name=" + cate_name
				+ ", cate_inventory=" + cate_inventory + ", cate_sale_count="
				+ cate_sale_count + ", cate_description=" + cate_description
				+ ", cate_oldprice=" + cate_oldprice + ", cate_price="
				+ cate_price + ", cate_image_id=" + cate_image_id
				+ ", category_id=" + category_id + ", flavor_category_id="
				+ flavor_category_id + ", cate_sale_info=" + cate_sale_info
				+ "]";
	}
	
}
