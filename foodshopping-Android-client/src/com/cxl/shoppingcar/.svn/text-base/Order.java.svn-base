package com.cxl.shoppingcar;

public class Order {
	String orderNo;// 订单号
	int userId;// 用户id
	String orderTime;//下单时间
	String amountPay;// 应付金额
	String deliveryMethod;// 配送方式
	int  recieverAddressId;//地址id
	String deliveryAdress;// 配送地址
	String paymentMethod;// 支付方式
	String coupon;// 优惠券
	boolean usable;// 可用
	String invoiceTitle;// 发票抬头
	String invoiceRemark;// 发票备注
	String tatal_price;// 商品总价
	String couponsDeduction;// 优惠抵扣券
	String deductibleRewards;// 积分抵扣
	String deliveryCosts;// 快递费用
	public String getOrderNo() {
		return orderNo;
	}
	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public String getOrderTime() {
		return orderTime;
	}
	public void setOrderTime(String orderTime) {
		this.orderTime = orderTime;
	}
	public String getAmountPay() {
		return amountPay;
	}
	public void setAmountPay(String amountPay) {
		this.amountPay = amountPay;
	}
	public String getDeliveryMethod() {
		return deliveryMethod;
	}
	public void setDeliveryMethod(String deliveryMethod) {
		this.deliveryMethod = deliveryMethod;
	}
	public int getRecieverAddressId() {
		return recieverAddressId;
	}
	public void setRecieverAddressId(int recieverAddressId) {
		this.recieverAddressId = recieverAddressId;
	}
	public String getDeliveryAdress() {
		return deliveryAdress;
	}
	public void setDeliveryAdress(String deliveryAdress) {
		this.deliveryAdress = deliveryAdress;
	}
	public String getPaymentMethod() {
		return paymentMethod;
	}
	public void setPaymentMethod(String paymentMethod) {
		this.paymentMethod = paymentMethod;
	}
	public String getCoupon() {
		return coupon;
	}
	public void setCoupon(String coupon) {
		this.coupon = coupon;
	}
	public boolean isUsable() {
		return usable;
	}
	public void setUsable(boolean usable) {
		this.usable = usable;
	}
	public String getInvoiceTitle() {
		return invoiceTitle;
	}
	public void setInvoiceTitle(String invoiceTitle) {
		this.invoiceTitle = invoiceTitle;
	}
	public String getInvoiceRemark() {
		return invoiceRemark;
	}
	public void setInvoiceRemark(String invoiceRemark) {
		this.invoiceRemark = invoiceRemark;
	}
	public String getTatal_price() {
		return tatal_price;
	}
	public void setTatal_price(String tatal_price) {
		this.tatal_price = tatal_price;
	}
	public String getCouponsDeduction() {
		return couponsDeduction;
	}
	public void setCouponsDeduction(String couponsDeduction) {
		this.couponsDeduction = couponsDeduction;
	}
	public String getDeductibleRewards() {
		return deductibleRewards;
	}
	public void setDeductibleRewards(String deductibleRewards) {
		this.deductibleRewards = deductibleRewards;
	}
	public String getDeliveryCosts() {
		return deliveryCosts;
	}
	public void setDeliveryCosts(String deliveryCosts) {
		this.deliveryCosts = deliveryCosts;
	}
	public Order(String orderNo, int userId, String orderTime,
			String amountPay, String deliveryMethod, int recieverAddressId,
			String deliveryAdress, String paymentMethod, String coupon,
			boolean usable, String invoiceTitle, String invoiceRemark,
			String tatal_price, String couponsDeduction,
			String deductibleRewards, String deliveryCosts) {
		super();
		this.orderNo = orderNo;
		this.userId = userId;
		this.orderTime = orderTime;
		this.amountPay = amountPay;
		this.deliveryMethod = deliveryMethod;
		this.recieverAddressId = recieverAddressId;
		this.deliveryAdress = deliveryAdress;
		this.paymentMethod = paymentMethod;
		this.coupon = coupon;
		this.usable = usable;
		this.invoiceTitle = invoiceTitle;
		this.invoiceRemark = invoiceRemark;
		this.tatal_price = tatal_price;
		this.couponsDeduction = couponsDeduction;
		this.deductibleRewards = deductibleRewards;
		this.deliveryCosts = deliveryCosts;
	}
	@Override
	public String toString() {
		return "Order [orderNo=" + orderNo + ", userId=" + userId
				+ ", orderTime=" + orderTime + ", amountPay=" + amountPay
				+ ", deliveryMethod=" + deliveryMethod + ", recieverAddressId="
				+ recieverAddressId + ", deliveryAdress=" + deliveryAdress
				+ ", paymentMethod=" + paymentMethod + ", coupon=" + coupon
				+ ", usable=" + usable + ", invoiceTitle=" + invoiceTitle
				+ ", invoiceRemark=" + invoiceRemark + ", tatal_price="
				+ tatal_price + ", couponsDeduction=" + couponsDeduction
				+ ", deductibleRewards=" + deductibleRewards
				+ ", deliveryCosts=" + deliveryCosts + "]";
	}
	
	

}
